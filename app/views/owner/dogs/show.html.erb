<div class='dogs show' data-currentUserId="<%=@current_user.id%>" data-recordId="<%=params[:id]%>">
  <%= render partial: 'dogs/profile_view', locals: {dog: @dog, pettings: @pettings}%>

  <div class="edit-delete-container">
    <section id="owner-editing-options">
      <%= link_to "Edit #{@dog.name}'s Profile", edit_owner_dog_path(@dog) %>
      <%= link_to "Delete #{@dog.name}'s Profile", owner_dog_path(@dog), method: :delete, data: { confirm: "Are you sure you want to delete #{@dog}'s profile?" }%>
    </section>
  </div>
</div>
